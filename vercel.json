{
    "version": 2,
    "builds": [
        {
            "src": "apps/web/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "apps/web/$1"
        }
    ],
    "functions": {
        "apps/web/**/*.{ts,tsx}": {
            "maxDuration": 10
        }
    },
    "headers": [
        {
            "source": "/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*).{js,css,woff,woff2,ttf,eot}",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*).{jpg,jpeg,png,gif,ico,svg,webp,avif}",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400, s-maxage=31536000"
                }
            ]
        }
    ]
}